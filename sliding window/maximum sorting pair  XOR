from typing import List


class Solution:
    def maximumXOR(self, nums: List[int]) -> int:
        result = 0
        for num in nums:
            result |= num
        return result

# Example usage
if __name__ == "__main__":
    nums = [3, 2, 5, 6, 8]
    solution = Solution()
    result = solution.maximumXOR(nums)
    print("Maximum XOR:", result)
